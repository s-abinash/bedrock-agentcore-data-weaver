# Backend core
AWS_REGION=us-west-2
S3_BUCKET_NAME=your-s3-bucket-name

# Bedrock Agent Runtime (required for /chat proxy and Bedrock deployment)
BEDROCK_AGENT_RUNTIME_ARN=arn:aws:bedrock-agentcore:us-west-2:123456789012:runtime/your-runtime-id
BEDROCK_AGENT_RUNTIME_SESSION_ID=
BEDROCK_AGENT_RUNTIME_QUALIFIER=DEFAULT

# Bedrock Code Interpreter (data_analyzer_agentcore.py)
CODE_INTERPRETER_TOOL_ID=ci-xxxxxxxx

# Local OpenAI override (fallback model used by get_bedrock_llm)
OPENAI_API_KEY=
OPENAI_API_VERSION=2025-01-01-preview

# Optional: default API base URL for the UI
BASE_URL=http://localhost:8080

# Observability / OTEL (populate if exporting traces)
OTEL_SERVICE_NAME=pandas-agent-core
OTEL_EXPORTER_OTLP_ENDPOINT=
